{
    "name": "thiio/stickyio",
    "description": "This is a SDK built in PHP for consuming stickyio api as detailed in https://sticky.io.com",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thiio\\Stickyio\\": "src/",
            "Thiio\\Stickyio\\Models\\": "src/models/",
            "Thiio\\Stickyio\\Requests\\": "src/requests/",
            "Thiio\\Stickyio\\Exceptions\\": "src/exceptions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thiio\\Stickyio\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Carlos Enrique Hoy Chi",
            "email": "enrique@thiio.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "~6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
